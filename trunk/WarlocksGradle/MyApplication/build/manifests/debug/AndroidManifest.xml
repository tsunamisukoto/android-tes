        <activity android:label="@string/title_activity_main" android:name="com.example.myapplication.WarlockGame" android:screenOrientation="landscape">

        </activity>
        <activity android:label="@string/title_activity_menu" android:name="com.example.myapplication.MenuActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
